#include <Stepper.h>

const int steps_per_rev = 200; // 200 steps = 360Â°
#define IN1 14
#define IN2 27
#define IN3 26
#define IN4 25

Stepper motor(steps_per_rev, IN1, IN2, IN3, IN4);

void setup() {
  motor.setSpeed(60);  // 60 RPM
  Serial.begin(115200);
}

void loop() {
  int steps_for_60_deg = 33; // ~60 degrees
  Serial.println("Rotating 60 degrees...");
  motor.step(steps_for_60_deg); // rotate forward only
  delay(10000); // wait for 10 seconds (10000 ms)
}
